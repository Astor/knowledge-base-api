{
  "name": "knowledge-base-api",
  "version": "1.0.0",
  "description": "Open AI Powered Knowledge Base API",
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "main": "./dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "tsup": "tsup",
    "build": "tsup",
    "start": "tsx -r dotenv/config src/index.ts",
    "lint": "eslint src",
    "lint:fix": "npm run lint --fix",
    "format": "prettier --write \"**/*.ts\"",
    "format:check": "prettier --list-different \"**/*.ts\""
  },
  "keywords": [
    "langchain",
    "node",
    "typescript",
    "llm"
  ],
  "author": "astordigital@gmail.com",
  "license": "MIT",
  "dependencies": {
    "@types/express": "^4.17.17",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "hnswlib-node": "^1.4.2",
    "langchain": "^0.0.81",
    "tsup": "^6.7.0"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.2",
    "@types/cors": "^2.8.17",
    "@types/js-yaml": "^4",
    "@types/node": "^18.15.5",
    "@types/pdf-parse": "^1.1.1",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "eslint": "^8.33.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "pdf-parse": "^1.1.1",
    "prettier": "^2.8.3",
    "tsx": "^3.12.3",
    "typescript": "^5.0.4"
  }
}
